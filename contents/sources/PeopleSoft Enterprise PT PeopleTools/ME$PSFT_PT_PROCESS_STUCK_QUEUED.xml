        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'PSFT_PT_PROCESS_STUCK_QUEUED' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, s.KEY_DATA FROM (Select 'Review ' || count(*) || ' Process(es) Stuck In Queued Status' AS KEY_DATA, 'Monitoring Service: Automatic SR created for: Investigate Too Many Stuck QUEUED Processes (Doc ID 2646138.1)' SOLUTION, 'Y' AS EVENT  From(  SELECT DISTINCT ('Instance = ' || PRCSINSTANCE || '; Process Name = ' || PRCSNAME || '; Type = ' || PRCSTYPE || '; PSJob = ' || MAINJOBNAME || '; PSJob Instance = ' || MAINJOBSEQ || '; PRCS Server = ' || SERVERNAMERQST) KEY_DATA,  'Investigate Too Many Stuck QUEUED Processes' ACTION,  'Y' AS EVENT  FROM SYSADM.PSPRCSQUE  WHERE PRCSSTARTDTTM IS NULL  AND RUNSTATUS=5  AND (RUNDTTM <= SYSTIMESTAMP)  )  having count(*) > 0) s]]>
        </SQLQuery>
